{
    "name": "turtel-chat",
    "private": true,
    "version": "1.0.0",
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "dev": "vite",
        "build": "rimraf ./dist/* && vite build",
        "express": "node express.mjs",
        "start": "npm run build && npm run express",
        "lint": "eslint . --ext .js,.ts",
        "lint-fix": "eslint . --ext .js,.ts --fix",
        "stylelint": "stylelint ./**/*.scss",
        "stylelint-fix": "stylelint ./**/*.scss --fix",
        "type-check": "tsc --noEmit",
        "lint-type-check": "npm run type-check && npm run lint && npm run stylelint",
        "format": "prettier --write .",
        "test": "mocha"
    },
    "devDependencies": {
        "@types/chai": "^4.3.18",
        "@types/mocha": "^10.0.7",
        "@types/sinon": "^17.0.3",
        "@types/sinon-chai": "^3.2.12",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "chai": "^5.1.1",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "handlebars": "^4.7.8",
        "husky": "8.0.3",
        "jsdom": "^25.0.0",
        "mocha": "^10.7.3",
        "prettier": "^3.3.2",
        "rimraf": "^6.0.1",
        "sass": "^1.77.5",
        "sass-loader": "^14.2.1",
        "sinon": "^18.0.0",
        "sinon-chai": "^4.0.0",
        "stylelint": "^16.6.1",
        "stylelint-config-standard": "^36.0.1",
        "stylelint-scss": "^6.3.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "vite": "^5.2.13"
    },
    "dependencies": {
        "express": "^4.19.2",
        "path": "^0.12.7"
    }
}
